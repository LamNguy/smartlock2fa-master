<%include partials/head.ejs%>

<div class="container" id="otpContainer" style="margin-top: 170px">
  <%
// var gmailBtnDis = gmailBtnEn ? '' : 'disabled';
// var smsBtnDis = smsBtnEn ? '' : 'disabled';
// var submitBtnDis = submitBtnEn ? '' : 'disabled';
// submitBtnDis='';
var gmailBtnDis = false;
var smsBtnDis = false;
var submitBtnDis = false;
var isActive = isActive;
%>

  <div class="row">
    <div class="col-md-4 col-md-offset-4 col-sm-12">
      <div class="panel panel-default">
        <p class="instruction">Authentication Method</p>
        <div class="panel-body">
          <div class="warning-message" id="<%=isActive%>">
            <p>Your account has not been activated.</p>
          </div>
          <button class='btn btn-lg btn-danger' id='gmailBtn' style="margin-right: 15px;" <%=gmailBtnDis%> ><i class="glyphicon glyphicon-envelope"></i>
            Gmail</button>
          <button class='btn btn-lg btn-success' id='smsBtn' style="margin-left: 15px;" <%=smsBtnDis%>><i class="glyphicon glyphicon-phone"></i>
            SMS</button>
        </div>
      </div>
    </div>
  </div>
</div>
